{
	"name": "davestewart/sketchpad",
	"type": "library",
	"description": "An innovative front-end environment for interactive Laravel development",
	"keywords": [
		"laravel",
		"testing",
		"tools",
		"tinker",
		"playground"
	],
	"homepage": "https://github.com/davestewart/laravel-sketchpad",
	"license": "MIT",
	"authors": [
		{
			"name": "Dave Stewart",
			"homepage": "http://davestewart.io",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "^5.0",
		"symfony/filesystem": "^3.1"
	},
	"autoload": {
		"psr-4": {
			"davestewart\\sketchpad\\": "src"
		}
	},
	"extra": {
        	"laravel": {
         		"providers": [
                		"davestewart\\sketchpad\\SketchpadServiceProvider"
            	        ]
		}
	}
}
